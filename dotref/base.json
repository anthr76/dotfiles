{
  "vars": {
    "git_profile": "foo"
  },
  "create": [
    {
      "name": "~/.config/git"
    },
    {
      "name": "~/.config/fish/dotfiles"
    }
  ],
  "link": [
    {
      "src": "sway",
      "dst": "~/.config/sway"
    },
    {
      "src": "lvim/config.lua",
      "dst": "~/.config/lvim/config.lua"
    },
    {
      "src": "foot",
      "dst": "~/.config/foot"
    },
    {
      "src": "fish/functions",
      "dst": "~/.config/fish/dotfiles/functions"
    },
    {
      "src": "fish/config.fish",
      "dst": "~/.config/fish/config.fish"
    },
    {
      "src": "fish/conf.d/source-functions.fish",
      "dst": "~/.config/fish/conf.d/source-functions.fish"
    },
    {
      "src": "container-bin",
      "dst": "~/.local/container-bin"
    },
    {
      "src": "environment.d",
      "dst": "~/.config/environment.d"
    },
    {
      "src": "ssh/config",
      "dst": "~/.ssh/config"
    }
  ]
